{
  "name": "jp-cli",
  "version": "1.4.2",
  "description": "Parse JSON on the commandline",
  "main": "index.js",
  "bin": {
    "jp": "bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/therealklanni/jp.git"
  },
  "scripts": {
    "build": "echo '#!/usr/bin/env node' > ./bin/index.js && babel --presets=es2015 index.js >> ./bin/index.js",
    "precommit": "lint-staged",
    "lint:fix": "eslint --fix --quiet --",
    "test": "cat package.json | node bin/index.js -hd0",
    "watch": "babel index.js --watch index.js --out-dir bin"
  },
  "keywords": [
    "json",
    "parse",
    "commandline",
    "sed"
  ],
  "author": "Kevin Lanni <therealklanni@gmail.com> (https://github.com/therealklanni)",
  "license": "MIT",
  "dependencies": {
    "concat-stream": "^1.5.2",
    "jsonpath": "^0.2.9",
    "lodash": "^4.17.2",
    "map-stream": "0.0.6",
    "split": "^1.0.0",
    "utf8-stream": "0.0.0",
    "yargs": "^6.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^1.7.0",
    "eslint-plugin-prettier": "^2.0.1",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.0",
    "prettier": "^1.2.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --quiet",
      "git add"
    ]
  }
}
